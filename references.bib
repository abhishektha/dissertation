month = {apr},
pages = {103024},
title = {{Effect of shaft stiffness and sole flexibility on perceived comfort and the plantar pressures generated when walking on a simulated underground coal mining surface}},
url = {http://www.sciencedirect.com/science/article/pii/S000368701830468X},
volume = {84},
