keywords = {Animation,Deformations,Morphing,Synthetic actors},
mendeley-groups = {Digital Human Modeling},
number = {3},
pages = {408--416},
title = {{SCAPE: Shape Completion and Animation of People}},
volume = {24},
year = {2005}
}

