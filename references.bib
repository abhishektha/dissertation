issn = {00219290},
journal = {Journal of Biomechanics},
keywords = {Boot shaft,Folder - Hiking,Gravel surface,Motion analysis,Stability,Walking},
language = {en},
mendeley-groups = {Shoes/Hiking},
mendeley-tags = {Boot shaft,Folder - Hiking,Gravel surface,Motion analysis,Stability,Walking},
