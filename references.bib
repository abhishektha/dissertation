doi = {10.1080/19424280.2013.834982},
file = {:Users/abhishektha/Library/Application Support/Mendeley Desktop/Downloaded/Barisch-Fritz et al. - 2014 - The effects of gender, age, and body mass on dynamic foot shape and foot deformation in children and adole.pdf:pdf},
