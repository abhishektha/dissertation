doi = {10.3357/AMHP.4650.2017},
file = {:C$\backslash$:/Users/abhis/Dropbox/References/Files/Cullinane, Rhodes, Stirling - 2017 - Mobility and agility during locomotion in the mark III space suit.pdf:pdf},
issn = {23756322},
